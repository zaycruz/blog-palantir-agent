{
  "name": "blog-palantir-agent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx src/index.ts",
    "cli": "tsx src/cli.ts",
    "hubspot:auth": "tsx src/hubspot-auth.ts",
    "build": "tsc -p tsconfig.build.json",
    "db:migrate": "tsx -e \"import { initializeDatabase } from './src/db/index.js'; initializeDatabase().then(() => console.log('Migrations complete')).catch(console.error)\"",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:regression": "vitest run tests/regression",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.14.11",
    "@vitest/coverage-v8": "^1.6.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vite-node": "^5.3.0",
    "vitest": "^1.6.0"
  },
  "vitest": {
    "include": [
      "tests/**/*.test.ts"
    ]
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.15",
    "@ai-sdk/openai": "^3.0.11",
    "@slack/bolt": "^4.6.0",
    "ai": "^6.0.37",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^17.2.3",
    "zod": "^4.3.5"
  }
}
